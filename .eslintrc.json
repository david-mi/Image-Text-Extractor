{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "no-duplicate-imports": [
      "warn",
      {
        "includeExports": true
      }
    ]
  }
}